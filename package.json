{
  "name": "nfeld.com",
  "version": "0.1.3",
  "author": "Nikolay Feldman<niko@nfeld.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:codeniko/nfeld.com.git"
  },
  "private": true,
  "scripts": {
    "start:js": "REACT_APP_VERSION=$npm_package_version NODE_ENV=development react-scripts start",
    "start": "npm-run-all -p watch:css start:js",
    "build": "./scripts/build.sh",
    "build:sitemap": "node scripts/sitemap-builder.js",
    "build:css": "node-sass-chokidar src/ -o src/",
    "watch:css": "npm run build:css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "test:components": "react-scripts test --env=jsdom",
    "test": "npm run lint && ENV=tests ./node_modules/.bin/_mocha -R spec ./test/**/*.js",
    "eject": "react-scripts eject",
    "lint": "eslint sr --ext .js --ext .jsx --ext .tsx --ext .ts",
    "deploy:functions": "firebase deploy --only functions",
    "deploy": "firebase deploy --only hosting",
    "clean": "rm -rf ./npm-debug.log ./build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "loglevel": "^1.6.8",
    "prop-types": "~15.7.2",
    "querystring": "~0.2.0",
    "raf": "^3.4.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-modal": "^3.11.2",
    "react-redux": "^7.2.1",
    "react-router": "~5.1.2",
    "react-router-dom": "~5.1.2",
    "simple-tracker": "~1.2.3",
    "typescript": "^3.9.7",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@babel/register": "^7.11.5",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.66",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^16.9.51",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/uuid": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "~10.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "~4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-react": "^7.21.3",
    "istanbul": "~1.0.0-alpha.2",
    "node-sass-chokidar": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "react-router-sitemap": "^1.2.0",
    "react-scripts": "^3.4.3",
    "rewire": "^5.0.0",
    "sinon": "^9.0.3",
    "xunit-file": "~1.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
