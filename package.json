{
  "name": "nfeld.com",
  "version": "0.1.0",
  "author": "Nikolay Feldman<niko@nfeld.com>",
  "repository": {
    "type": "git",
    "url": "git@github.com:codeniko/nfeld.com.git"
  },
  "private": true,
  "scripts": {
    "start:js": "NODE_ENV=development react-scripts start",
    "start": "npm-run-all -p watch:css start:js",
    "build": "./scripts/build.sh",
    "build:sitemap": "node scripts/sitemap-builder.js",
    "build:css": "node-sass-chokidar src/ -o src/",
    "watch:css": "npm run build:css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "test:components": "react-scripts test --env=jsdom",
    "test": "npm run lint && ENV=tests ./node_modules/.bin/_mocha -R spec ./test/**/*.js",
    "eject": "react-scripts eject",
    "lint": "eslint src test *.js --ext .js --ext .jsx",
    "deploy:functions": "firebase deploy --only functions",
    "deploy": "firebase deploy --only hosting",
    "clean": "rm -rf ./npm-debug.log ./build"
  },
  "dependencies": {
    "classnames": "~2.2.5",
    "loglevel": "~1.6.1",
    "prop-types": "~15.6.0",
    "querystring": "~0.2.0",
    "react": "~16.2.0",
    "react-dom": "~16.2.0",
    "react-router": "~3.2.0",
    "simple-tracker": "~1.0.0",
    "uuid": "~3.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "~3.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-react": "^7.5.1",
    "istanbul": "~1.0.0-alpha.2",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "react-router-sitemap": "^1.1.3",
    "react-scripts": "1.1.1",
    "rewire": "^2.5.2",
    "sinon": "~1.17.4",
    "xunit-file": "~1.0.0"
  }
}
